<con:custom-adapter-config xmlns:con="http://www.cgi.com/eai/adapter/custom/telecom/config">
    <con:version>1.0</con:version>
    <!--Optional:-->
    <con:communication-type>
        <con:rv>true</con:rv>
        <con:jms>false</con:jms>
    </con:communication-type>
    <!--Optional:-->
    <con:jms-descriptor>
        <con:context>com.tibco.tibjms.naming.TibjmsInitialContextFactory</con:context>
        <con:queueFactory>QueueConnectionFactory</con:queueFactory>
        <con:topicFactory>TopicConnectionFactory</con:topicFactory>
        <!--Optional:-->
        <con:login>cadapter</con:login>
        <!--Optional:-->
        <con:password>cadapter</con:password>
        <con:url>tcp://localhost:7222</con:url>
        <!--Optional:-->
        <con:jndi_login>cadapter</con:jndi_login>
        <!--Optional:-->
        <con:jndi_password>cadapter</con:jndi_password>
    </con:jms-descriptor>
    <!--Optional:-->
    <con:tibrv-descriptor>
        <!--Optional:-->
        <con:service>7500</con:service>
        <!--Optional:-->
        <con:daemon>tcp:7500</con:daemon>
    </con:tibrv-descriptor>
    <!--Optional:-->
    <con:tibhawkrv-descriptor>
        <con:monitor-enabled>true</con:monitor-enabled>
        <!--Optional:-->
        <con:service>7474</con:service>
        <!--Optional:-->
        <con:daemon>tcp:7474</con:daemon>
        <con:micro-agent-name>WeatherProbes</con:micro-agent-name>
    </con:tibhawkrv-descriptor>
    <!--Optional:-->
    <con:subject-descriptor>
        <con:subject>customAdapter</con:subject>
    </con:subject-descriptor>
    <!--Optional:-->
    <con:threads-descriptor>
        <con:threadsNo>0</con:threadsNo>
    </con:threads-descriptor>
    <con:plugin-def-prop>
        system=WheatherProbes
        snmp-version=1
        ip-address=127.0.0.1
        port=161
        community=Public
        request-type=GET
        oid-name=getStatus
        retries-count=2
        timeout=100
    </con:plugin-def-prop>
</con:custom-adapter-config>