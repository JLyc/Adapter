        <con:service>7445</con:service>
        <!--Optional:-->
        <con:daemon>tcp:7445</con:daemon>
        <con:micro-agent-name>WeatherProbe_Adapter</con:micro-agent-name>
        <!--<con:micro-agent-name>U2000_Adapter</con:micro-agent-name>-->
